{
  "name": "agile-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:admin": "cross-env MODULE_CONFIG=./vue.entry.admin.js vue-cli-service build",
    "build:mobile": "cross-env MODULE_CONFIG=./vue.entry.mobile.js vue-cli-service build",
    "inspect:admin": "cross-env MODULE_CONFIG=./vue.entry.admin.js vue-cli-service inspect --mode production > webpack.entry.admin.js",
    "inspect:mobile": "cross-env MODULE_CONFIG=./vue.entry.mobile.js vue-cli-service inspect --mode production > webpack.entry.mobile.js",
    "serve:admin": "cross-env MODULE_CONFIG=./vue.entry.admin.js vue-cli-service serve",
    "serve:mobile": "cross-env MODULE_CONFIG=./vue.entry.mobile.js vue-cli-service serve"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^4.0.0",
    "axios": "^0.21.1",
    "axios-extensions": "^3.1.3",
    "axios-progress-bar": "^1.2.0",
    "core-js": "^3.8.1",
    "element-plus": "^1.0.1-beta.14",
    "perfect-scrollbar": "^1.5.0",
    "query-string": "^6.13.7",
    "register-service-worker": "^1.7.2",
    "vue": "^3.0.2",
    "vue-context": "^6.0.0",
    "vue-cropperjs": "^4.2.0",
    "vue-router": "^4.0.0-rc.5",
    "vue-router-back-button": "^1.3.0",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-pwa": "^4.5.8",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "assign-deep": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.12.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-vue": "^7.3.0",
    "less": "^4.0.0",
    "less-loader": "^7.2.0",
    "mocker-api": "^2.7.1",
    "prettier": "^2.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
